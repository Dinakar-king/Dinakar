    <script>
        function showContent(page) {
            const content = {
                home: "<h2>Home</h2><p>Welcome to the homepage of the company.</p>",
                products: "<h2>Faculty</h2><p>Here you can find our faculty detailss.</p>",
                services: "<h2>Services</h2><p>We offer various services for our customers.</p>",
                branches: "<h2>Branches</h2><p>Here are the locations of our branches.</p>",
                about: "<h2>About Us</h2><p>Learn more about our company and mission.</p>"
            };

            document.getElementById('center').innerHTML = content[page] || "<h2>Welcome to our website!</h2><p>Click on the links on the left to see more content.</p>";
        }
    </script>

